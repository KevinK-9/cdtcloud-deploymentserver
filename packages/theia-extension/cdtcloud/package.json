{
  "private": true,
  "name": "cdtcloud",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.5.0",
    "@theia/core": "^1.21.0",
    "@theia/editor": "^1.21.0",
    "@theia/filesystem": "^1.21.0",
    "@theia/output": "^1.21.0",
    "@theia/search-in-workspace": "^1.21.0",
    "@theia/workspace": "^1.21.0",
    "@types/form-data": "^2.5.0",
    "axios": "^0.24.0",
    "got": "^11.0.0",
    "react-select": "^5.2.1",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.14.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.0"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "tsc -w --preserveWatchOutput",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc -noEmit"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/cdtcloud-frontend-module",
      "backend": "lib/node/cdtcloud-backend-module"
    }
  ],
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-builtin-cpp": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/cpp-1.39.1-prel.vsix"
  }
}
